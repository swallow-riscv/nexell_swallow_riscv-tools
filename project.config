[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/Hyunseok Jung (aronjung)
	owner = group user/Sungwoo Park (NexellBackUp)
[access "refs/for/refs/*"]
	read = group user/Sungwoo Park (NexellBackUp)
	push = group user/Sungwoo Park (NexellBackUp)
	label-Code-Review = -2..+2 group user/Sungwoo Park (NexellBackUp)
	label-Verified = -1..+1 group user/Sungwoo Park (NexellBackUp)
	rebase = group user/Sungwoo Park (NexellBackUp)
	removeReviewer = group user/Sungwoo Park (NexellBackUp)
	submit = group user/Sungwoo Park (NexellBackUp)
[access "refs/heads/*"]
	read = group user/Sungwoo Park (NexellBackUp)
	create = group user/Sungwoo Park (NexellBackUp)
	push = +force group user/Sungwoo Park (NexellBackUp)
	pushMerge = group user/Sungwoo Park (NexellBackUp)
[access "refs/tags/*"]
	push = group user/Sungwoo Park (NexellBackUp)
	push = +force group user/Sungwoo Park (NexellBackUp)
[receive]
	requireSignedOffBy = true
	requireChangeId = true
[submit]
	action = rebase always
	mergeContent = false
